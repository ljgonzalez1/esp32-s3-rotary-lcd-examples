{
  "name": "ESP32-S3 N16R8 (custom)",
  "vendor": "Custom",
  "url": "https://docs.espressif.com/",
  "platform": "espressif32",
  "frameworks": ["arduino"],
  "build": {
    "core": "arduino",
    "mcu": "esp32s3",
    "variant": "esp32s3",
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "flash_size": "16MB",
    "arduino": {
      "memory_type": "qio_opi"
    },
    "psram_type": "opi"
  },
  "upload": {
    "maximum_size": 16777216,
    "maximum_ram_size": 327680,
    "require_upload_port": true,
    "wait_for_upload_port": true
  },
  "debug": {
    "openocd_target": "esp32s3.cfg"
  },
  "connectivity": ["wifi","bluetooth"]
}
